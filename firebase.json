{
    "flutter": {
        "platforms": {
            "ios": {
                "buildConfigurations": {
                    "Debug": {
                        "projectId": "teambuilder-plus-fe74d",
                        "appId": "1:312163687148:ios:9db93e08c23a25583763c9",
                        "uploadDebugSymbols": false,
                        "fileOutput": "ios/Runner/GoogleService-Info.plist"
                    }
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "teambuilder-plus-fe74d",
                    "configurations": {
                        "ios": "1:312163687148:ios:9db93e08c23a25583763c9"
                    }
                }
            }
        }
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "functions": {
        "source": "functions"
    },
    "emulators": {
        "auth": {
            "port": 9099
        },
        "functions": {
            "port": 5050,
            "host": "localhost"
        },
        "firestore": {
            "port": 8585
        },
        "ui": {
            "enabled": true,
            "port": 4050
        },
        "singleProjectMode": true
    },
    "hosting": [
        {
            "target": "main",
            "public": "web",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "cleanUrls": true,
            "trailingSlash": false,
            "headers": [
                {
                    "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|woff|woff2|ttf|eot)",
                    "headers": [
                        {
                            "key": "Cache-Control",
                            "value": "max-age=31536000"
                        }
                    ]
                }
            ]
        },
        {
            "target": "info",
            "public": "web-info",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "cleanUrls": true,
            "trailingSlash": false
        }
    ]
}
